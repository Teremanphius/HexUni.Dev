Button = Animations:CreateButton({SectionParent=SectionAnimpacks,Name="Toy",Interact="Click",Callback=function()
	Character.Animate.climb.ClimbAnim.AnimationId = "http://www.roblox.com/asset/?id=782843869";
	Character.Animate.fall.FallAnim.AnimationId = "http://www.roblox.com/asset/?id=782846423";
	Character.Animate.idle.Animation1.AnimationId = "http://www.roblox.com/asset/?id=782841498";
