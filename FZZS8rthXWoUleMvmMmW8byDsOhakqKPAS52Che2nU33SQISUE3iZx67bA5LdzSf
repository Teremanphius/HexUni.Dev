				returnedPlayer = game:GetService("ReplicatedStorage").RemoteFunction:InvokeServer(unpack(args));
				troopText = "Troops: ";
				for i, v in pairs(returnedPlayer.Troops) do
					if (i ~= 1) then
						troopText = troopText .. ", ";
					end
					troopText = troopText .. v;
				end
				Troops:Set(troopText);
			end
		end
	end);
