	for i, v in pairs(speaker.Backpack:GetChildren()) do
		if v:IsA("Tool") then
			v:Destroy();
		end
	end
	effecttrue();
	NSUI:Notify({Title="Removed!",Content="Removed Gear's",Duration=3,Image=7733715400});
