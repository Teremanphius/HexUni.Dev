						pcall(function()
							getRoot(speaker.Character).CFrame = foundPlr.Character.HumanoidRootPart.CFrame * CFrame.Angles(0, math.rad(180), 0) * CFrame.new(0, 2, 0.5);
							getRoot(speaker.Character).AssemblyLinearVelocity = Vector3.new();
						end);
					end);
				end
			end
		end
	end);
