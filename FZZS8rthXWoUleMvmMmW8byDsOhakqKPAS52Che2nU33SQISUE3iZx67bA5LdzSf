			for i, v in next, game:GetService("Players"):GetPlayers() do
				if string.find(v.Name:lower(), name:lower()) then
					x = v;
					break;
				end
			end
			return x;
		end
		bangingfoundplrv2v2 = getPlr(plrName);
		if bangingfoundplrv2v2 then
			flwnum = -1;
