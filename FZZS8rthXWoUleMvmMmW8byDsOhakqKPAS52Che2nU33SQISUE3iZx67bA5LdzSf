Toggle = ESPTAB:CreateToggle({SectionParent=Sectionesp,Name="Main ESP",CurrentValue=false,Callback=function(ESP)
	pcall(function()
		espfunc = ESP;
		if espfunc then
			Sense:Load();
		end
		if not espfunc then
			Sense:Unload();
		end
	end);
end});
