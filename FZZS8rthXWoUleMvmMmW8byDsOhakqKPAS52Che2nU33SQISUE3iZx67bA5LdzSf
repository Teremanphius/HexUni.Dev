		if (key:lower() == freezekeybind) then
			freezeanchor = not freezeanchor;
			for _, v in pairs(speaker.Character:GetChildren()) do
				if v:IsA("BasePart") then
					v.Anchored = freezeanchor;
				end
			end
		end
	end);
