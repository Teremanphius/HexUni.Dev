local spinaround = false;
local radiusfromplayer = 10;
local spinSpeed = 1;
local angle = 0;
Input = Misc:CreateInput({SectionParent=Sectionplayrerelatedtp,Name="Spin Around",PlaceholderText="Player",RemoveTextAfterFocusLost=false,Callback=function(Text)
	pcall(function()
		plrName = Text;
		SpinaroundPlr = getPlr(plrName);
		if SpinaroundPlr then
			if ((spinaround == true) or (SpinaroundPlr == speaker)) then
				effectfalse();
				notifyyw("No Longer Spining Around The Player", "Not Spining Around " .. SpinaroundPlr.Name .. " Anymore", 2, 10747384394);
				spinaround = false;
				return;
			else
				spinaround = true;
				effecttrue();
				notifyyw("Now Spining Around", "Spining Around " .. SpinaroundPlr.Name, 2, 10709790644);
				while spinaround do
					game:GetService("RunService").Heartbeat:Wait();
					angle = angle + (spinSpeed * game:GetService("RunService").Heartbeat:Wait());
					if (angle > (2 * math.pi)) then
						angle = angle - (2 * math.pi);
					end
					local offset = Vector3.new(math.cos(angle) * radiusfromplayer, 0, math.sin(angle) * radiusfromplayer);
					local newCFrame = CFrame.new(SpinaroundPlr.Character.HumanoidRootPart.Position + offset, SpinaroundPlr.Character.HumanoidRootPart.Position);
					Character.HumanoidRootPart.CFrame = newCFrame;
				end
			end
		else
			notifyyw("Player Does Not Exist!", "Bro Does Not Exist!", 2, 10747384394);
			effectfalse();
		end
	end);
end});
SpinaroundSliderSpeed = Misc:CreateSlider({SectionParent=Sectionplayrerelatedtp,Name="Spin Around Speed",Range={1,99},Increment=1,Suffix="Speed",CurrentValue=spinSpeed,Callback=function(Value)
	pcall(function()
		spinSpeed = Value;
	end);
end});
