	Sliderwalk:Set(16);
	if not r15(speaker) then
		Sliderhip:Set(0);
		speaker.Character.Humanoid.HipHeight = 0;
	else
		Sliderhip:Set(2);
		speaker.Character.Humanoid.HipHeight = 2;
	end
	if speaker.Character:FindFirstChildOfClass("Humanoid").UseJumpPower then
		Sliderjpower:Set(50);
	else
		Sliderjpower:Set(7);
	end
	Slidergrav:Set(196);
end});
