	if KeyBind then
		function speakerFrontflip()
			speaker.Character.Humanoid:ChangeState("Jumping");
			task.wait();
			speaker.Character.Humanoid.Sit = true;
			for i = 1, 360 do
				delay(i / 720, function()
					speaker.Character.Humanoid.Sit = true;
					getRoot(speaker.Character).CFrame = getRoot(speaker.Character).CFrame * CFrame.Angles(-hbackflipnfrontflip, 0, 0);
				end);
			end
			task.wait(0.55);
			speaker.Character.Humanoid.Sit = false;
		end
		speakerFrontflip();
	end
end});
