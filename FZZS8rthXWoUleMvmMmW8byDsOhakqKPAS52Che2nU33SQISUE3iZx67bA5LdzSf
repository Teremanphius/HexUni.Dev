	Character.Animate.jump.JumpAnim.AnimationId = "http://www.roblox.com/asset/?id=16738336650";
	Character.Animate.run.RunAnim.AnimationId = "http://www.roblox.com/asset/?id=16738337225";
	Character.Animate.swim.Swim.AnimationId = "http://www.roblox.com/asset/?id=16738339158";
