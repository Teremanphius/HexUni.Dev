	nohurtov = Value
	if nohurtov then
		hurtOverlay = true
		CoreGui.TopBar.FullScreenFrame.HurtOverlay:GetPropertyChangedSignal("Visible"):Connect(function()
			if (hurtOverlay == true) then
				CoreGui.TopBar.FullScreenFrame.HurtOverlay.Visible = false
			end
		end)
	else
		hurtOverlay = false
	end
end})

