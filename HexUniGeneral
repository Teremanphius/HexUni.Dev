	pcall(function()
		plrName = Option:match("^(.-) %(")
		foundPlr = getPlr(plrName)
		if foundPlr then
			if (closeorbitingfuck == true) then
				sfxSound("false")
				CloseOrbititngLoop:Disconnect()
