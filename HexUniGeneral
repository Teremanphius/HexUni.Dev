		cframewalk = Value
		if cframewalk then
			CFramewalkloop = RunService.RenderStepped:Connect(function()
				getRoot(speaker.Character).CFrame = getRoot(speaker.Character).CFrame + (Humanoid.MoveDirection * CFramevaluespeed)
			end)
		else
			CFramewalkloop:Disconnect()
		end
end})

